{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "UseBreakpoint": {
      "type": "object",
      "properties": {
        "key": {
          "anyOf": [
            {
              "$ref": "#/definitions/Breakpoint"
            },
            {
              "type": "null"
            }
          ],
          "description": "Current breakpoint key: `xs`, `sm`, `md`, `lg`, `xl`"
        },
        "isMobile": {
          "type": "boolean",
          "description": "true if the viewport is mobile"
        },
        "isTablet": {
          "type": "boolean",
          "description": "true if the viewport is tablet"
        },
        "isSmallDesktop": {
          "type": "boolean",
          "description": "true if the viewport is small desktop"
        },
        "isDesktop": {
          "type": "boolean",
          "description": "true if the viewport is desktop"
        },
        "isLargeDesktop": {
          "type": "boolean",
          "description": "true if the viewport is large desktop"
        }
      },
      "required": ["key", "isMobile", "isTablet", "isSmallDesktop", "isDesktop", "isLargeDesktop"],
      "additionalProperties": false
    },
    "Breakpoint": {
      "type": "string",
      "enum": ["xs", "sm", "md", "lg", "xl"]
    },
    "NamedParameters<typeof useBreakpoint>": {
      "type": "object",
      "properties": {
        "rules": {
          "$ref": "#/definitions/BreakpointsRules",
          "description": "={} - Custom breakpoints rules"
        }
      },
      "additionalProperties": false
    },
    "BreakpointsRules": {
      "type": "object",
      "properties": {
        "xs": {
          "type": "number"
        },
        "sm": {
          "type": "number"
        },
        "md": {
          "type": "number"
        },
        "lg": {
          "type": "number"
        },
        "xl": {
          "type": "number"
        }
      },
      "additionalProperties": false
    }
  }
}
