{
  "name": "@react-jopau/components",
  "version": "0.18.1",
  "description": "Common components for React projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joel3112/react-jopau.git"
  },
  "author": "Joel Paucarima Franco <joel.dev.3112@outlook.es>",
  "license": "MIT",
  "homepage": "https://github.com/joel3112/react-jopau/tree/main/packages/components#readme",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typesVersions": {
    "*": {
      "./ui": [
        "./dist/ui.d.ts"
      ],
      "./contexts": [
        "./dist/contexts.d.ts"
      ]
    }
  },
  "exports": {
    "./ui": {
      "import": "./dist/esm/ui/index.js",
      "require": "./dist/cjs/ui/index.js"
    },
    "./contexts": {
      "import": "./dist/esm/contexts/index.js",
      "require": "./dist/cjs/contexts/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "test": "jest --config=../../jest.config.js --selectProjects=@react-jopau/components"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nextui-org/react": "1.0.0-beta.10",
    "@react-aria/focus": "^3.10.1",
    "@react-aria/utils": "^3.14.2",
    "@react-jopau/styles": "^0.18.1",
    "@react-jopau/utils": "^0.18.1",
    "@stitches/react": "^1.2.8",
    "classnames": "^2.3.2",
    "react-hotkeys-hook": "^4.3.0",
    "react-icons": "^4.6.0",
    "react-is": "^18.2.0",
    "react-text-mask": "^5.5.0"
  },
  "peerDependencies": {
    "@types/react": "^17.0.0 || ^18.0.0",
    "@types/react-dom": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@react-jopau/shared": "^0.18.1",
    "@types/react-is": "^17.0.3",
    "@types/react-text-mask": "^5.4.11",
    "ts-jest": "^29.0.1",
    "typescript": "^4.8.4"
  }
}
